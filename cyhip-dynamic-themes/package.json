{
    "name": "cyhip-dynamic-themes",
    "version": "0.1.1",
    "description": "Tailwind-powered dynamic color themes for React apps.",
    "author": "@KassioRF, @CyberHippie-io",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/CyberHippie-io/cyhip-dynamic-themes"
    },
